datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

model Url {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  redirect    String
  url         String   @unique
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
